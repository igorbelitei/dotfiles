<gconfentryfile>
  <entrylist base="/desktop/gnome/interface">
    <entry>
      <key>font_name</key>
      <schema_key>/schemas/desktop/gnome/interface/font_name</schema_key>
      <value>
        <string>Droid Sans 10</string>
      </value>
    </entry>
    <entry>
      <key>document_font_name</key>
      <schema_key>/schemas/desktop/gnome/interface/document_font_name</schema_key>
      <value>
        <string>Droid Sans 10</string>
      </value>
    </entry>
    <entry>
      <key>monospace_font_name</key>
      <schema_key>/schemas/desktop/gnome/interface/monospace_font_name</schema_key>
      <value>
        <string>Inconsolata Medium 12</string>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/metacity/general">
    <entry>
      <key>titlebar_font</key>
      <schema_key>/schemas/apps/metacity/general/titlebar_font</schema_key>
      <value>
        <string>Droid Sans Bold 11</string>
      </value>
    </entry>
  </entrylist>
  <entrylist base="/apps/nautilus">
    <entry>
      <key>preferences/desktop_font</key>
      <schema_key>/schemas/apps/nautilus/preferences/desktop_font</schema_key>
      <value>
        <string>Droid Sans 11</string>
      </value>
    </entry>
  </entrylist>
</gconfentryfile>

